{
  "name": "miniengine",
  "version": "0.0.0",
  "description": "A web based, minimalist game engine.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "format": "prettier --write \"src/**/*.ts\" --single-quote --trailing-comma es5",
    "prebuild": "npm run format",
    "build": "npm run clean && webpack --config ./webpack.prod.js",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "assets": "cd ./src/public && pwa-asset-generator ./icon.svg ./resources/images -i ./index.html -m ./manifest.json --background \"#000\""
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.6.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.18.2",
    "pwa-asset-generator": "^3.2.3",
    "sass-loader": "^8.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {}
}
